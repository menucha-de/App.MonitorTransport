<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:havis.net.ui.shared.client" xmlns:e="urn:import:havis.app.monitortransport.ui.client">
	<ui:style>
		.core-ui {
			display: -webkit-flex;
			display: -ms-flexbox;
			display: flex;
			-webkit-flex-direction: column;
			-ms-flex-direction: column;
			flex-direction: column;
			font-size: 1.5em;
		}
		
		.configurations {
			font-size: 1.5em;
			overflow: auto;
			-webkit-flex: 1;
			-ms-flex: 1;
			flex: 1;
		}
		
		.header {
			line-height: 3em;
			font-weight: bold;

		}
	</ui:style>
	<g:FlowPanel ui:field="container" addStyleNames="{style.core-ui}">
		<g:Label text="Event Configuration" styleName="{style.header}" />
		<e:EventSetList ui:field="eventSetList" />
	</g:FlowPanel>
</ui:UiBinder> 